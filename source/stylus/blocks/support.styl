.support
  display flex
  flex-direction column
  justify-content center
  align-items center

.support__phone
  padding 0
  margin 0
  margin-bottom 5px
  font-family "Raleway Bold", Arial, sans-serif
  font-size 18px
  color #0d3351
  text-decoration none

  &:visited
    color #0d3351

.support__support
  padding 0
  margin 0
  margin-bottom 10px
  font-size 12px
  color #0d3351

.support__copyright
  padding 0
  margin 0
  font-size 16px
  color #0d3351